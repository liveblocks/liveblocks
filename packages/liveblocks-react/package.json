{
  "name": "@liveblocks/react",
  "version": "0.17.8-dev",
  "description": "A set of React hooks and providers to use Liveblocks declaratively.",
  "main": "./index.cjs",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "files": [
    "**"
  ],
  "keywords": [
    "react",
    "liveblocks",
    "multiplayer",
    "live-cursors",
    "collaborative"
  ],
  "bugs": {
    "url": "https://github.com/liveblocks/liveblocks/issues"
  },
  "scripts": {
    "build": "tsup && ../../scripts/build.sh",
    "start": "tsup --watch",
    "format": "eslint --fix src/ && prettier --write src/",
    "lint": "eslint src/",
    "test": "jest --watch --silent --verbose",
    "test-ci": "jest --silent --verbose",
    "codegen": "tsc scripts/generate-compat.ts && node scripts/generate-compat.js && eslint --fix src/compat.tsx && prettier --write src/compat.tsx"
  },
  "license": "Apache-2.0",
  "peerDependencies": {
    "@liveblocks/client": "0.17.8",
    "react": "^16.14.0 || ^17 || ^18"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@swc/core": "^1.2.233",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^13.1.1",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "msw": "^0.27.1",
    "react-error-boundary": "^3.1.1",
    "ts-node": "^10.8.1",
    "tsup": "^6.2.2",
    "typescript": "^4.7.3",
    "whatwg-fetch": "^3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-react"
  },
  "sideEffects": false
}
