{
  "name": "@liveblocks/infer-schema",
  "version": "0.0.10",
  "description": "Helper to infer a Liveblocks schema from plain LSON data",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "format": "eslint --fix src/ ; prettier --write src/",
    "test": "jest --verbose --no-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/liveblocks/liveblocks-schema.git"
  },
  "keywords": [
    "liveblocks",
    "schema",
    "validation",
    "language",
    "parser"
  ],
  "devDependencies": {
    "@liveblocks/eslint-config": "*",
    "@liveblocks/jest-config": "*",
    "@types/pluralize": "^0.0.29",
    "fast-check": "^3.7.0",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@liveblocks/core": "^1.0.0",
    "@liveblocks/schema": "0.0.10",
    "decoders": "^2.0.3",
    "pluralize": "^8.0.0"
  },
  "sideEffects": false
}
