{
  "name": "@liveblocks/node",
  "version": "0.18.3",
  "description": "A server-side utility that lets you set up a Liveblocks authentication endpoint.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md"
  ],
  "keywords": [
    "node",
    "liveblocks",
    "multiplayer",
    "live-cursors",
    "collaborative"
  ],
  "bugs": {
    "url": "https://github.com/liveblocks/liveblocks/issues"
  },
  "scripts": {
    "dev": "tsup --watch --onSuccess ../../scripts/build.sh",
    "build": "tsup && ../../scripts/build.sh",
    "format": "eslint --fix src/ && prettier --write src/",
    "lint": "eslint src/",
    "test": "jest --watch --silent --verbose",
    "test-ci": "jest --silent --verbose --passWithNoTests"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node-fetch": "^2.5.8",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.1.2",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.2.2",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-node"
  }
}
