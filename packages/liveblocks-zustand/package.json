{
  "name": "@liveblocks/zustand",
  "version": "0.18.3",
  "description": "A middleware to integrate Liveblocks into Zustand stores.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "**"
  ],
  "keywords": [
    "zustand",
    "react",
    "liveblocks",
    "multiplayer",
    "live-cursors",
    "collaborative"
  ],
  "scripts": {
    "build": "tsup && ../../scripts/build.sh",
    "format": "eslint --fix src/ && prettier --write src/",
    "lint": "eslint src/",
    "test": "jest --watch --silent --verbose",
    "test-ci": "jest --silent --verbose",
    "dtslint": "dtslint --localTs node_modules/typescript/lib --expectOnly types"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-zustand"
  },
  "peerDependencies": {
    "@liveblocks/client": "0.18.3",
    "zustand": "^3"
  },
  "devDependencies": {
    "@definitelytyped/dtslint": "^0.0.103",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.1.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "msw": "^0.36.4",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.2.2",
    "typescript": "^4.7.2",
    "whatwg-fetch": "^3.6.2",
    "zustand": "^3.6.9"
  },
  "sideEffects": false
}
