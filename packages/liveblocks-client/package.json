  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-client"
  },
  "homepage": "https://liveblocks.io",
  "keywords": [
    "liveblocks",
    "real-time",
    "toolkit",
    "multiplayer",
    "websockets",
    "collaboration",
