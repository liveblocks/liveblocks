{
  "name": "@liveblocks/client",
  "version": "0.18.3",
  "description": "A client that lets you interact with Liveblocks servers.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md"
  ],
  "keywords": [
    "liveblocks",
    "multiplayer",
    "live-cursors",
    "collaborative"
  ],
  "bugs": {
    "url": "https://github.com/liveblocks/liveblocks/issues"
  },
  "scripts": {
    "dev": "tsup --watch --onSuccess ../../scripts/build.sh",
    "build": "tsup && ../../scripts/build.sh",
    "format": "eslint --fix src/ && prettier --write src/",
    "lint": "eslint src/",
    "test": "jest --watch --silent --verbose --config=./jest.config.js",
    "test-ci": "jest --silent --verbose --passWithNoTests"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@liveblocks/core": "0.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.1.2",
    "@types/node-fetch": "^2.6.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fast-check": "^3.0.1",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "msw": "^0.39.1",
    "node-fetch": "2.6.7",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tsup": "^6.2.2",
    "typescript": "^4.7.2",
    "whatwg-fetch": "^3.6.2",
    "ws": "^8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-client"
  },
  "sideEffects": false
}
