{
  "name": "@liveblocks/client",
  "version": "0.18.1-dev",
  "description": "A client that lets you interact with Liveblocks servers.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    "./internal": {
      "require": "./internal.js",
      "import": "./internal.mjs",
      "types": "./internal.d.ts"
    },
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    }
  },
  "files": [
    "**"
  ],
  "keywords": [
    "liveblocks",
    "multiplayer",
    "live-cursors",
    "collaborative"
  ],
  "bugs": {
    "url": "https://github.com/liveblocks/liveblocks/issues"
  },
  "scripts": {
    "build": "tsup && ../../scripts/build.sh",
    "format": "eslint --fix src/ && prettier --write src/",
    "lint": "eslint src/",
    "test": "jest --watch --silent --verbose --config=./jest.config.js",
    "test-ci": "jest --silent --verbose",
    "test-e2e": "jest --silent --verbose --config=./jest.config.e2e.js"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@swc/core": "^1.2.233",
    "@types/jest": "^26.0.24",
    "@types/node-fetch": "^2.6.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fast-check": "^3.0.1",
    "jest": "^28.0.3",
    "jest-each": "^27.5.1",
    "jest-environment-jsdom": "^28.1.0",
    "msw": "^0.39.1",
    "node-fetch": "2.6.7",
    "prettier": "^2.7.1",
    "regenerator-runtime": "^0.13.9",
    "tsup": "^6.2.2",
    "typescript": "^4.7.2",
    "whatwg-fetch": "^3.6.2",
    "ws": "^8.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-client"
  },
  "sideEffects": false
}
