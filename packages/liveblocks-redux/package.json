{
  "name": "@liveblocks/redux",
  "version": "0.18.3",
  "description": "A store enhancer to integrate Liveblocks into Redux stores.",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md"
  ],
  "keywords": [
    "redux",
    "react",
    "liveblocks",
    "multiplayer",
    "live-cursors",
    "collaborative"
  ],
  "scripts": {
    "dev": "tsup --watch --onSuccess ../../scripts/build.sh",
    "build": "tsup && ../../scripts/build.sh",
    "format": "eslint --fix src/ && prettier --write src/",
    "lint": "eslint src/",
    "test": "jest --silent --verbose",
    "test:watch": "jest --silent --verbose --watch",
    "dtslint": "dtslint --localTs node_modules/typescript/lib --expectOnly types"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/liveblocks/liveblocks.git",
    "directory": "packages/liveblocks-redux"
  },
  "dependencies": {
    "@liveblocks/client": "0.18.3",
    "@liveblocks/core": "0.18.3"
  },
  "peerDependencies": {
    "redux": "^4"
  },
  "devDependencies": {
    "@definitelytyped/dtslint": "^0.0.103",
    "@liveblocks/eslint-config": "*",
    "@reduxjs/toolkit": "^1.7.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.1.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "msw": "^0.36.4",
    "redux": "^4.1.2",
    "ts-jest": "^29.0.3",
    "whatwg-fetch": "^3.6.2"
  },
  "sideEffects": false
}
