                      "data": [
                        {
                          "type": "room",
                          "id": "HTOGSiXcORTECjfNBBLii",
                          "lastConnectionAt": "2022-08-08T23:23:15.281Z",
                          "createdAt": "2022-08-08T23:23:15.281Z",
                          "metadata": {
                            "name": [
                              "My room"
                            ],
                            "type": [
                              "whiteboard"
                            ]
                          },
                          "defaultAccesses": [
                            "room:write"
                          ],
                          "groupsAccesses": {
                            "product": [
                              "room:write"
                            ]
                          },
                          "usersAccesses": {
                            "vinod": [
                              "room:write"
                            ]
