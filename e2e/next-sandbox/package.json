    "@liveblocks/zustand": "*",
    "@reduxjs/toolkit": "^1.9.7",
    "encoding": "^0.1.13",
    "next": "^12.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "zustand": "^4.1.3"
  },
  "devDependencies": {
    "@liveblocks/eslint-config": "*",
    "@liveblocks/jest-config": "*",
    "@playwright/test": "^1.38.1",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.199",
    "lodash": "^4.17.21"
